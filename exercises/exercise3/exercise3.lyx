\begin_layout Section*
Equations
\end_layout

\begin_layout Standard
Drag coefficient
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
C_{D}=\frac{D}{\frac{1}{2}\rho V^{2}A}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $D$
\end_inset

 is lift force, 
\begin_inset Formula $\rho$
\end_inset

 is density, 
\begin_inset Formula $V$
\end_inset

 is air speed, and 
\begin_inset Formula $A$
\end_inset

 is wing area.
 Similarly, the lift coefficient is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
C_{L}=\frac{L}{\frac{1}{2}\rho V^{2}A}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $L$
\end_inset

 is lift force.
\end_layout

